<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <property name="now" value="now()" dbms="mysql"/>

    <changeSet id="insert-default-user-role" author="Katerina">
        <insert tableName="user_role">
            <column name="user_id">1</column>
            <column name="role_id">1</column>
            <column name="create_date" valueDate="${now}"/>
        </insert>
        <insert tableName="user_role">
            <column name="user_id">1</column>
            <column name="role_id">2</column>
            <column name="create_date" valueDate="${now}"/>
        </insert>
        <insert tableName="user_role">
            <column name="user_id">1</column>
            <column name="role_id">3</column>
            <column name="create_date" valueDate="${now}"/>
        </insert>
        <insert tableName="user_role">
            <column name="user_id">2</column>
            <column name="role_id">2</column>
            <column name="create_date" valueDate="${now}"/>
        </insert>
        <insert tableName="user_role">
            <column name="user_id">3</column>
            <column name="role_id">3</column>
            <column name="create_date" valueDate="${now}"/>
        </insert>
    </changeSet>

</databaseChangeLog>